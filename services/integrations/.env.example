# Auth0 Configuration
AUTH0_DOMAIN=your-domain.auth0.com
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret
AUTH0_AUDIENCE=your-api-identifier
AUTH0_SCOPE=openid profile email
AUTH0_CONNECTION=Username-Password-Authentication

# Twilio Configuration
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_API_KEY=your-api-key
TWILIO_API_SECRET=your-api-secret
TWILIO_ROOM_TYPE=group

# Stripe Configuration
STRIPE_SECRET_KEY=your-secret-key
STRIPE_PUBLIC_KEY=your-public-key
STRIPE_WEBHOOK_SECRET=your-webhook-secret
STRIPE_CURRENCY=brl
STRIPE_PAYMENT_METHODS=["card", "boleto", "pix"]

# SendGrid Configuration
SENDGRID_API_KEY=your-api-key
SENDGRID_FROM_EMAIL=no-reply@sanara.com.br
SENDGRID_FROM_NAME=Sanara Healthcare
SENDGRID_TEMPLATES={"welcome": "d-template-id", "reset_password": "d-template-id"}

# Environment
NODE_ENV=development
LOG_LEVEL=debug 